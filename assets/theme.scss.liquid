       display: block;
    position: relative;
    padding: 10px 10px;

      >a{
        padding:10px 30px;
        background:$menu_background_color;

      }
      &:hover{
        ul.dropdown-menu{
          display: block !important;
        }
      }
      >ul.dropdown-menu{
        position: absolute;
        left: 100%;
        top: 0;
        display: none;
        background-color:$menu_background_color_dropdown;
